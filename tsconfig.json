{
    "compilerOptions": {
        "extends": "@vue/tsconfig/tsconfig.json",
        "isolatedModules": true,
        "paths": {
            "@/*": ["./src/*"],
        },
        "vetur.validation.script": false,
        "allowJs": true,
        "moduleResolution": "NodeNext",
        "target": "ES2020",
        "module": "ESNext",
        "lib": ["ESNext", "DOM", "DOM.Iterable", "ESNext.String", "ESNext.Array"]
    }
}